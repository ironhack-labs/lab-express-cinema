<div class="container">
  <div class="row">
    <a href="/movies">Go back</a>
    {{#each mId}}
    <div class="col-md-4">
      <div class="element-card">
        <div class="img-box">
          <img class="img-fluid text-center" src="{{this.image}}" />
        </div>
        <div class="element-caption text-center">
          <h4 class="text-center">{{this.title}}</h4>
          <h5 class="text-center">{{this.director}}</h5>
          <ul>
            {{#each this.stars}}
            <li>{{this}}</li>
            {{/each}}
          </ul>
          <p class="text-center">{{this.description}}</p>
          <p class="text-center">{{this.showtimes}}</p>
   
          
        </div>
      </div>
    </div>
    {{/each}}
    
  </div>
</div>