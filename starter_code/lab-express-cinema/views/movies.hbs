<div class="container">
  <div class="row">
    {{#each movies}}
    <div class="col-md-4">
      <div class="element-card">
        <div class="img-box">
          <img class="img-fluid text-center" src="{{this.image}}" />
        </div>
        <div class="element-caption text-center">
          <h4 class="text-center">{{this.title}}</h4>
          <a href="/movies/{{id}}" class="btn btn-primary">See more</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>