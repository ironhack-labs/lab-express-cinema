<h1>Ironhack Cinema</h1>
<h3>Click on the movie to check the showtimes!</h3>
<section class="movies">
    {{#each movies}}
      <div class="peli">
        <img src="{{this.image}}" alt="{{this.title}}">
        <h4>{{this.title}}</h4>
        <a href="/movies/movie-detail/{{this.id}}">See more</a>
      </div>  
    {{/each}}
</section>