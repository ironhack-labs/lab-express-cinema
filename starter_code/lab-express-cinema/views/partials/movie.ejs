<article class = "movie-container">
  <img src = "<%= movie.image %>" class = "movie-poster" alt = "<%= movie.title %> Movie Poster">
  <h2 class = "movie-title"><%= movie.title %></h2>
  <a href = "/movies/<%= movie._id %>" class = "movie-link">See More</a>
  <a href = "/movies/new" class = "movie-link">Add</a>
  <a href = "/movies/<%= movie._id %>/edit" class = "movie-link">Edit</a>
  <form action="/movies/<%= movie._id %>/delete" method="post">
    <input type="submit" value = "Delete">
  </form>
</article>