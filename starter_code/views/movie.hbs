<div>
  {{#each movie}}
    <img src="{{this.image}}" alt="{{this.title}}">
    <h2>{{this.title}}</h2>
    <p><strong>Director: </strong>{{this.director}}</p>
    <p><strong>Stars:</strong></p>
    {{#each this.stars}}
      <li>{{this}}</li>
    {{/each}}
    <p>{{this.description}}</p>
    <p>{{this.showtimes}}</p>
  {{/each}}
</div>