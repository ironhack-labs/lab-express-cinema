
<div>
    <img src="{{movie.image}}">
    <a href="/movies">Go Back</a>
    <h1>{{movie.title}}</h1>
    <h2>Director: {{movie.director}}</h2>
    Stars
    <ul>
    {{#each movie.stars}}
        <li>{{this}}</li>
    {{/each}}
    </ul>
    <p>{{movie.description}}</p>
    {{#each movie.showtimes}}
        {{this}} |
    {{/each}}
</div>
