<section id="section-details">
<article>
<img src="{{this.image}}" alt="image">
<div>
<a href="/movies">Go back</a>
<h2>{{this.title}}</h2>
<h3>Director: {{this.director}}</h3>
<h4>Stars</h4>
<ul>
{{#each this.stars as |star|}}
<li>{{star}}</li>
{{/each}}
</ul>
<p>{{this.description}}</p>
<p><b>| 
{{#each this.showtimes as |stimes|}}
{{stimes}} |
{{/each}}
</b>
</p>
</div>

</article>




</section>





