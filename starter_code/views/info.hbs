<section id="more-info-container">
    <img src="{{movie.image}}">
    <div id="movie-info">
        <a id="see-more" href="/movies">Go back</a>
        <h2 id="movie-title">{{movie.title}}</h2>
        <section id="info-highlight">
        <h3 id="movie-director">Director: {{movie.director}}</h3>
        <h4>Stars:</h4>
        <ul>
            {{#each movie.stars}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>{{movie.description}}</p>
        <p>
            {{#each movie.showtimes}}
              <a>{{this}} | </a>
            {{/each}}
        </p>
        </section>
    </div>
</section>