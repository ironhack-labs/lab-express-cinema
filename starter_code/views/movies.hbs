<h1>Ironhack Cinema</h1>
<p>Click on the movies to check the showtimes</p>

<div class="giant-movies-div">

    {{#each moviesList}}
    <div class="modular-movies-div">
        <img src={{this.image}} alt="">
        <h3>{{this.title}}</h3>
        <a href="/movies/{{this._id}}">See more</a>
    </div>
    {{/each}}
</div>