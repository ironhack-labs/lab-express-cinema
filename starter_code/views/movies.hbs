<div class='container'>
    <h1>Ironhack cinema</h1>
    <p>Click on the movies to check the showtimes!</p>
    <div class='movies-container'>
        {{#each movies}}
            <div class='movie-card'>
                <img src="{{this.image}}" alt="movie folder">
                <h3>{{this.title}}</h3>
                <a href="/movie/{{this._id}}">See more</a>
            </div>
        {{/each}}
    </div>
</div>