<div class = 'allMovies'>

    <header>
        <h2>Ironhack Cinema</h2>
        <p>Click on the movie to check the showtimes!</p>
    </header>

    <div>

        {{#each movieList}}

        <div class="movie">
            <img src='{{image}}'>

            <h3>{{title}}</h3>

            <p><a href="/movies/{{_id}}">See more</a></p>

        </div>

        {{/each}}

    </div>






</div>