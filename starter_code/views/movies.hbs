<h1 class="movies-text">Ironhack Cinema</h1>
<p class="movies-text info">Click on the movie to check the show times</p>
<div class=all-movies>
    {{#each movies}}
    <div class="one-movie">
        <img src={{this.image}}></img>
        <p>{{this.title}}</p>
        <p><a href="/movie/{{this._id}}">See more</a></p>
    </div>
{{/each}}
</div>


