
<div class="row">
{{#each movies}}
<div class="card" style="width: 18rem;">
  <img src="{{this.image}}" class="card-img-top">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <a href="/movie/{{this._id}}" class="btn btn-primary">See more</a>
  </div>
</div>
{{/each}}
</div>
