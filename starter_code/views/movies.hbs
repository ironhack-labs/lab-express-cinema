<header class="movies-header">
    <h1>Ironhack Cinema</h1>
    <p>Click on the movie to check the showtimes!</p>
</header>

<div class="flex-container">
    {{#each movies}}
        <div class="movie-card">
            <img src="{{image}}">
            <h2>{{title}}</h2>
            <p><a href="/movies/{{_id}}">See More</a></p>
        </div>
    {{/each}}
</div>