<h1>IronHack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>

<div class="containerMovie">
    <div class="row">
        {{#each movie}}
        <div class="col-md-3">
            <img src={{image}}>
            <p>{{title}}</p>
            <a href="/movies/{{_id}} "> See More</a>
            <br>
            <br>
        </div>
        {{/each}}
    </div>
</div> 