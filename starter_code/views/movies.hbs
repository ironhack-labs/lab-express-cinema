

<div class="container">
    <h2 class="my-2 py-2"> <b>Ironhack Cinema</b></h2>
    <h4 class="mb-3 py-2">Click on the movie to check the showtimes!</h4>
    <div class="row">
        {{#each movies}}
        <div class="col-4 text-center p-3">
            <img src="{{this.image}}" alt="movie-image">
            <h5 class="m-3">{{this.title}}</h5>
            <p><a href="/movies/{{_id}}">See more</a></p>
        </div>
        {{/each}}
    </div>
</div>