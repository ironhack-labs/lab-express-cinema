   <h1>Ironhack Cinema</h1>
   <h3>Click on the movie to check the showtimes!<h3>  
       <div class="movies-container">          
       {{#each movies}}
           <div class="movie">
               <img src="{{image}}" alt="movie-poster">
               <p>{{title}}</p>
               <p><a class="see" href="/movies/{{id}}">See more</a></p>
           </div>
           {{/each}}
</div>

