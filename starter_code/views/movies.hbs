<h1>Ironhack Cinema</h1>
<h4>Click on the movie to check the showtimes</h4>

<div class="list">
    {{#each movieArray}}
    <div class="one-list">
        
            <img src="{{this.image}}" alt="{{this.title}}" />
            <p>{{this.title}}</p>
            <a href="/movie/{{this._id}}">See More</a>
        
    </div>
{{/each}}
</div>