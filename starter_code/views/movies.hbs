<h1>Ironhack Cinema</h1>

<p>Click on the movie to check the showtimes</p>

<container class="movie_list">
    <ul>
        {{#each movieid}}
        <li>
            <img src="{{this.image}}">
            <h2>{{this.title}}</h2>
            <a href="/movies/{{this.id}}">See more</a>
        </li>
        {{/each}}
    </ul>
</container>