<div class="title">
<h1>Ironhack Cinema</h1>
<h4>Click on the movie to check the showtime</h4>
</div>
<div class="container1">
    {{#each movies}}
    <div class="column">
        <img src={{this.image}} alt="">
        <h6>{{this.title}}</h6>
        <a href="/movie/{{this._id}}">
            <p>See more</p>
        </a>
    </div>
    {{/each}}
</div>