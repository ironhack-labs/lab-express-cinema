<div class="movies-container">
    {{#each movies}}
    <div>
        <div>
            <img src="{{this.image}}" alt="{{this.name}}">
        </div>
        <div>
            <h3>{{this.title}}</h3>
            <a href="/movie-details/{{this._id}}">See more</a>
        </div>
    </div>
    {{/each}}
</div>