<section class="list-section">

  <h2>{{title}}</h2>
  <h6>Click on the movie to check the showtimes!</h6>

  <div class="movie-div">
    {{#each moviesArray}}
      <div class="col-lg-4 artist-div">
        <img src='{{this.image}}' />
        <h6>{{this.title}}</h6>
        <a href='/movies/{{this._id}}'>See More</a>
      </div>
    {{/each}}
  </div>

</section>