
<section id="movieList">
    <h1>Ironhack Cinema</h1>
    <h2>Click on the movie to check the showtimes!</h2>
    <div id="seccionPelis">
        {{#each movies}}
        <div>
            <img src="{{this.image}}" alt="{{this.image}}">
            <h3>{{this.title}}</h3>
            <a href="/movies/{{this._id}}">See more</a>
        </div>
        {{/each}}
    </div>
</section>



