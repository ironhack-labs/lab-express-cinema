<h1>Movies</h1>
<div class="movies">
    {{#each moviesList}}

    <div class="moviecard">
        <div>
            <h3>{{this.title}}</h3>
        </div>
        <div>
            <img src="{{this.image}}" alt="Image">
        </div>

        <div>
            <a href="/movies/{{_id}}">See more</a>
        </div>
    </div>



    {{/each}}

</div>