<h1 class="movies-title">Movies</h1>
<section class="movies-container">
  <a class="go-back-btn" href="/">‚Üê</a>
  {{#each allMovies}}
    <a href="/movies/{{this._id}}">
      <div class="movie-card">
        <h2>{{this.title}}</h2>
        <img src="{{this.image}}" alt="movie banner">
      </div>
    </a>
  {{/each}}
</section>