<main id="movies">
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check showtimes!</p>
  <div id="movie-results">
    {{#each movies}}
    <div class="movie">
      <img src="{{this.image}}" />
      <div class="movie-info">
        <h2>{{this.title}}</h2>
        <a class="button" href="movie/{{this.id}}">See more</a>
      </div>
    </div>
    {{/each}}
  </div>
</main>
