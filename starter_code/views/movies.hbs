<section>
  <h2>Ironhack Cinema</h2>
  <p>Click on the movie to check the showtimes!</p>
  <a href="/">Go Back</a>


  <div id="movies-list">
    {{#each movies}}
    <div class="content-film">
      <img src="{{image}}" alt="">
      <h3>{{title}}</h3>
      <a href="/movies/{{_id}}">See more</a>
    </div>
    {{/each}}
  </div>
</section>