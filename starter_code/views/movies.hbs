<div class="tittle">
  <h2>IronHack Cinema</h2>
  <h5>Click on the movie to check the showtimes!</h3>
</div>

<div class="cards">
  {{#each movies}}
  <div class="card" style="width: 18rem;">
    <img src="{{this.image}}" class="card-img-top" alt="...">
    <div class="card-body">
      <p class="card-text">{{this.title}}</p>
      <a href="movie/{{this.id}}">See more</a>
    </div>
  </div>
  {{/each}}
</div>