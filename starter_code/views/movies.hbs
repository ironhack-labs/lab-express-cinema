<section class="uk-section back-sect uk-padding-small">
  <div class="uk-container uk-container-expand">
    <h1 class="uk-text-primary">Ironhack Cinema</h1>
    <h3 class="uk-text-primary">Click on the movie to check the showtimes</h3>
  </div>
</section>
<hr class="uk-divider-icon">
<section class="uk-section uk-section-muted">
  <div class="uk-container">
    <div class="uk-child-width-1-3@m uk-child-width-1-2@s uk-text-center uk-grid-match" uk-grid>
      {{#each movies as |movie|}}
      {{> movieCard movie}}
      {{/each}}
    </div>
  </div>
</section>