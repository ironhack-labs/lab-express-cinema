<div id="top">
    <h1>Ironhack Cinema</h1>
    <h3>Click on the movie to check the showtimes!</h3>
</div>
<div id="all">
    {{#each movies as |movie|}}
        <div id="movie">
            <img src="{{movie.image}}" alt="">
            <p>{{movie.title}}</p>
            <a href="movie/{{movie._id}}">See more</a>
        </div>
    {{/each}}
</div>