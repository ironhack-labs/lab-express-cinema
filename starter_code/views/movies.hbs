<div id="movies-section">
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check the showtimes!</p>
  <div class="movies-list">
    {{#each movies}}
    <div class="movie">
      <img src="{{this.image}}" alt="{{this.title}}">
      <h2>{{this.title}}</h2>
      <a href="/movie/{{this._id}}">See more</a>
    </div>
    {{/each}}
  </div>
</div>