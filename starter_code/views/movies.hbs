
<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes! </p>
<div class="gallery">

    {{#each movies}}
    <div class="card">
        <img src="{{this.image}}" alt="">
        <p class="movie-title">{{this.title}}</p>
        <a href="/movie/{{this._id}}">See more</a>
    </div>
    {{/each}}
</div>