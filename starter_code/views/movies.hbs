<h2>Ironhack Cinema</h2>
<h3>Click on the movie to check the showtimes</h3>
<div class="movies">
  {{#each movies}}
  <div class="movie">
    <p><a href="/movie/{{this._id}}"><img src="{{this.image}}"></img></a></p>
    <p>{{this.title}}</p>
    <a href="/movie/{{this._id}}">See more...</a>
  </div>
  {{/each}}
</div>