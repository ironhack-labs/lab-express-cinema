<h2>Ironhack Cinema</h2>
<p>Click on the movie to check the showtimes!</p>
<ul>
  {{#each movies }}
  <img src="{{ this.image }}" alt="">
  <p>{{ this.title }}</p>
  <li><a href="/movies/detail/{{this._id}}">See more</a></li>
  <form action="/movies/{{this._id}}/delete" method="POST"><button>Delete</button></form>
  {{/each}}
</ul>