<div class="container">
<img src="{{movie.image}}" alt="">
<div class="movieInfo">
<a href="/movies">Go back</a>
<h1>{{movie.title}}</h1>
<h2>Director: {{movie.director}}</h2>
<h3>Stars</h3>
<ul>
  {{#each movie.stars}}
  <li>{{this}}</li>
  {{/each}}
</ul>
<p>{{movie.description}}</p>
{{#each movie.showtimes}}
<span> {{this}} |</span>
{{/each}}
</div>
</div>