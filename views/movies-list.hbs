<h2>Ironhack Cinema</h2>
<h3>Click on the movie to check the showtimes!</h3>

<div id="movies">
    {{#each movies}}
    <div class="movie">
        <img src={{this.image}} >
        <div>{{this.description}} </div>
        <a href=/movies/details/{{this._id}} >see more</a>
        <a href=/movies/edit/{{this._id}} >edit movie</a>
        <a href=/movies/delete/{{this._id}} >delete movie</a>
    </div>
    {{/each}}
 </div>