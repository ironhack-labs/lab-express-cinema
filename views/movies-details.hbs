

<img src="{{ movie.image }}" alt="">
<a href="/movies">Go back</a>
        <h2>{{ movie.title }}</h2>
        <h4>Director: {{ movie.director }}</h4>
        <h4>Stars</h4>
        <ul>
            {{#each movie.stars}}
                <li>{{ this }}</li>
            {{/each}}
        </ul>
        <p>{{ movie.description }}</p>

        <p>
            {{#each movie.showtimes}}
            <span>{{ this }} | </span>
            {{/each}}
        </p>