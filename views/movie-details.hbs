<div class='movie-cover'>
  <img src="{{image}}" alt='' />
</div>
<div class='movie-details'>
  {{title}}
  {{director}}
  <h4>
    Starts
  </h4>
  <ul>
    {{#each stars}}
      <li class='star-item'>
        {{this}}
      </li>
    {{/each}}
  </ul>
  <p class='description'>
    {{description}}
  </p>
  <div class='showtime-items'>
    {{#each showtimes}}
      <p>
        {{this}} |
      </p>
    {{/each}}
  </div>
</div>