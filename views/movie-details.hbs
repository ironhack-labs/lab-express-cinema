<button><a href="/movies">Go back</a></button>

<img src="{{movie.image}}"  alt="Movie thumbnail">
<h2>{{movie.title}}</h2> 
<h3>Director: {{movie.director}}</h3>
<h4>Stars</h4>
<ul>
    {{#each movie.stars}}
        <li><p>{{this}}</p></li>
    {{/each}}
</ul>
<p>{{movie.description}}</p>
<p>
    {{#each movie.showtimes}}
        {{this}} |
    {{/each}}
</p>