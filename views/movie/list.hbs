<div class="container">
  <div class="row d-flex flex-row m-3">
    {{#each movies}}
    <div class="col-lg-4 artist-container d-flex flex-column align-items-center">
      <img src="{{image}}" width="150px" class="m-2">
      <p>{{title}}</p>
      <a href="movie/{{_id}}">See more</a>
    </div>
    {{/each}}
  </div>
</div>
