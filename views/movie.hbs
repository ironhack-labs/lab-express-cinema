<img class="movie-img" src="{{movie.image}}">
<div class="movie-text">
  <a href="/movies">Go back</a>
  <h1>{{movie.title}}</h1>
  <h2>Director: {{movie.director}}</h2>
  <h2>Stars</h2>
  <ul>
    {{#each movie.stars}}
    <li>{{this}}</li>
    {{/each}}
  </ul>
  <p>{{movie.description}}</p>
  <h3>{{movie.times}}</h3>
</div>