<h1>Ironhack Cinema</h1>
<h2>Click on the movie to check the showtimes!</h2>
{{#each allMovies}}
<div class="movie-wrapper">
    <div class="movie-item">
        <div class="movie-cover">
            <img src="{{this.image}}" alt="{{this.title}}">
        </div>
        <div class="movie-text">
            <p>{{this.title}}</p>
            <a href="/movies/{{this._id}}" title="See more">See more</a>
        </div>
    </div>
</div>
{{/each}}