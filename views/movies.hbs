<div class="all-movies">
<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>
<div class="movie-wrapper">
        {{#each allMoviesDB}}
        <div class="oneMovie">
            <img src="{{this.image}}" alt="{{this.title}}">
            <h2>{{this.title}}</h2>
            <a href="/movies/{{this._id}}">See More</a>
        </div>
        {{/each}}
</div>
</div>