<h1>Iron Cinema</h1>
<p>Click on the movie to check the showtimes!</p>
<div class="container text-center">
    <div class="row">
        {{#each movies}}
        <div class="container">
            <img src="{{image}}" alt="movie">
            <div>
                <h5 class="card-title">{{title}}</h5>
                <a class="btn btn-primary" href="/details/{{_id}}">See More</a>
            </div>
        </div>
        {{/each}}
    </div>
</div>