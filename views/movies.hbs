<div class="container">
    <h1>Ironhack Cinema</h1>
    <h2>Click on the movie to check the showtimes!</h2>

    <div class="row">
        {{#each movies}}
        <div class="col-md-4 text-center pb-4">
            <img src="{{image}}">
            <h4>{{title}}</h4>
            <a href="/movies/{{_id}}">See more</a>
        </div>
        {{/each}}
    </div>
</div>