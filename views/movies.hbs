{{!-- Titles | images displaying| See more that points to each movie --}}

<div >
    {{#each movies}}
    <div class="uk-card uk-card-default">
        <div class="uk-card-media-top">
            <img src="{{image}}" alt="">
        </div>
        <div class="uk-card-body">
            <h3 class="uk-card-title">{{title}}</h3>
            <p>{{description}}</p>
            <a href="/movies/{{_id}}">See more</a>
        </div>
    </div>
    {{/each}}
</div>