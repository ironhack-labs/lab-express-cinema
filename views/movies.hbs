<div class="container">
	<h2>Ironhack Cinema</h2>
	<h4 class="click">Click on the movie to check the showtimes!</h4>
	<a href="/">Go Back</a>
</div>	
<div class="movieLi">
	{{#each allMovies}}
		<div class="movieDet">
			<a href="/movies/{{this._id}}"><img class="img-fluid" src="{{this.image}}" alt=""></a>
			<h4>{{this.title}}<h4>
			<a href="/movies/{{this._id}}" class="see-more">See more</a>
		</div>
	{{/each}}				
</div>





