<div class="movies-intro">
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check the showtimes!</p>
</div>
<div class="movies">
  {{#each movies}}
    <div class="movie">
      <img src="{{image}}" alt="" />
      {{title}}
      <a href="/movies/{{_id}}">See more</a>
    </div>
  {{/each}}
</div>