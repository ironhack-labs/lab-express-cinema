<section id="movie-list-section">
  <h1>Ironhack Cinema</h1>
  <h2>Click on the movie to check the showtimes!</h2>
  <div id="movie-list">
    {{#each movies}}
      <div class="movie-div">
        <img src={{image}} alt="movie image" />
        <h3>{{title}}</h3>
        <a href="/movies/{{_id}}">See more</a>
      </div>
    {{/each}}
  </div>
</section>