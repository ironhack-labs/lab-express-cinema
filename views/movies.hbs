<div class="moviesTitle">
    <h1>Ironhack Cinema</h1>
    <h3>Click on the movie to check the showtimes!</h3>
</div>

<div class="moviesDiv">

    {{#each movies}}

    <div class="movieCard">
        <img src="{{image}}" alt="{{title}}">
        <h2>{{title}}</h2>
        <a href="/movie/{{_id}}">See more</a>
    </div>


    {{/each}}
</div>