<h1>Ironhack Cinema</h1>
<h2><b>Click on the movies to check the showtimes!</b></h2>
<div class="col-sm-4">
    {{#each movies}}
    <div class="">
        <img src="{{this.image}}" alt="Imagen">
        <p>{{this.title}}</p>
        <a href="/movies/{{_id}}">See more</a>
    </div>
</div>
{{/each}}

