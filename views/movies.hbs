<head>
    <title>Movie Catalog</title>
</head>
<body>
    <h1>Movie Catalog</h1>
    <div class="movie-container">
        {{#each movies}}
            <div class="movie-card">
                <img class="movie-image" src="{{this.image}}" alt="{{this.title}}">
                <h2 class="movie-title">{{this.title}}</h2>
                <a href="/movies/{{this._id}}">See More</a>
            </div>
        {{/each}}
    </div>
</body>
</html>
