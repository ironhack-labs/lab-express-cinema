
    <div class="container">
        <div class="init">
            <h1>Ironhack Cinema</h2>
            <h4>Click on the movie to check the showtimes!</h4>
        </div>
        <div class="row">
            {{#each movies}}
            <div class="col-4 movies">
                <img src="{{image}}" alt="image film">
                <h3>{{title}}</h3>
                <a href="/movies/details/{{id}}"> <h6>See more</h6></a>
            </div>
            {{/each}}
        </div>
    </div>
