<button><a href="/movies/form">Create Movie!</a></button>

<section  class="movies-list">
    {{#each movies}}
        <div>
            <img src="{{this.image}}" alt="{{this.name}}-image">
            <h2>{{this.title}}</h2>
            <a href="/details/{{this._id}}">See more</a>
        </div>
    {{/each}}
</section>
    