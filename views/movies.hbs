<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>

<ul id="movie-list">
    {{#each movies}}
    <li class="item">
        <img src="{{this.image}}" alt="{{this.title}}"><br>
        <p class="title">Title : {{this.title}}</p>
        <a href="/movie/{{this._id}}/">See more</a>
    </li>
    {{/each}}
</ul>