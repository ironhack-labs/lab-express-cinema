<main id='movies'>
	<h1>Movies here</h1>

	<div id='all-movies-wrapper'>
		{{#each movies}}
			<div id='movie-tile'>
				<img src='{{this.image}}' alt='' />
				<h2>{{this.title}}</h2>
				<a href='/movie-detail'>See more</a>

			</div>
		{{/each}}
	</div>
</main>

