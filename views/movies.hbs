<div class="movies-page">

  <h1>Ironhack Cinema</h1>
  <br />
  <h2>Chick on the movie to check the showtimes!</h2>
  <br />

  <div class="movies-git">
    {{#each moviesArray}}

      <div class="card">
        <img src="{{image}}" alt={{title}} class="image" />
        <h3>{{title}}</h3>
        <a href="/movies/{{_id}}">See more</a>
      </div>

    {{/each}}
  </div>
</div>