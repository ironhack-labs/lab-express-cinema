<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>

<div class="all-movies-container">
    {{#each movies}}
        <div class="movie-container">
            <img src="{{this.image}}" alt="">
            <p>{{this.title}}</p>
            <a href="/movies/{{this._id}}">See more</a>
        </div>
    {{/each}}
</div>
