<main id='movies'>
	<h1>Ironhack Cinema</h1>

	<p>Click on the Movie to check the showtimes!</p>

	<div id='all-movies-wrapper'>
		{{#each movies}}
			<div id='movie-tile'>
				<img src='{{this.image}}' alt='' />
				<h2>{{this.title}}</h2>
				<a href='/movies/{{this._id}}'>See more</a>

			</div>
		{{/each}}
	</div>
</main>

