<div class="movies-page">
  <h1>Ironhack Cinema</h1>
  <h2>Click on the movie to check the showtimes!</h2>
  <div class="movie-rosters">
    {{#each movies}}
      <div class="movie-roster">
        <img src="{{image}}" alt="movie image" />
        <h3>{{title}}</h3>
        <a href="/movies/{{_id}}">See more</a>
      </div>
    {{/each}}
  </div>
</div>