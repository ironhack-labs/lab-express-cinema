<div class="container">

    <div class="movies-title">
        <h2>Ironhack Cinema</h2>
    </div>
    <div class="card-wrapper">
        {{#each allMovies}}
        <div class="card-movie">
            <div class="card-img">
                <a href="/detail/{{_id}}"><img src="{{this.image}}" alt="{{this.title}}"></a>
            </div>
            <p>{{this.title}}</p>
            <a href="/detail/{{_id}}">See More</a>
        </div>
        {{/each}}

    </div>
</div>