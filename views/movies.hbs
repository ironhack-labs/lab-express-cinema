<div>
    <div class="container">
  <div class="row">
            {{#each movie}}
      <div class="movie-card" style="margin: 5%; text-align:center;" >
          <img src="{{this.image}}" alt="Image Movie">
          <h4 style="padding-left: 5%;">{{this.title}}</h4>

          <button type="button" class="btn btn-light btn-lg">
            <a href="/movies/{{_id}}">See more</a>
          </button>
      </div>
      {{/each}}
</div>


  </div>