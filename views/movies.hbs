<h1>IRONHACK CINEMA</h1>
<hr>

<div class="row">
    {{#each movies}}
    <div class="col-md-4 mb-4">
        <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="{{image}}" alt="">
            <div class="card-body">
                <p class="card-text">{{title}}</p>
                <a href="/details/{{_id}}">See more</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>