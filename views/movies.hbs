

    


<article class="container">

    <div class="row">
        <div class="col-md-12">
        <h1>Ironhack Cinema</h1>
        <p>Click on the movie to check the showtimes!</p>
    </div>

    </div>
    
    <div class="row movies-list">

        {{#each movies}}

            <div class="col-md-4 movie-list">

                <img class="shadow" src="{{image}}" alt="{{title}}">
                <h4>{{title}}</h4>
                <a href="/movies/{{id}}">See more</a>

            </div>

        {{/each}}

    </div>
</article>