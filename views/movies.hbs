<h1>Movies list</h1>
<div class="row">
{{#each movies}}
<div class="card col-3" style="width: 18rem;">
  <img src="{{this.image}}" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <a href="/{{this._id}}" class="btn btn-primary">See more</a>
  </div>
</div>
{{/each}}
</div>