<div class="movies-header">
  <h2>Ironhack Cinema</h2>
  <h3>Click on the movie to check the showtimes!</h3>
</div>

<div class="container">
  <div class="row">
        {{#each movies}}
      <div class="col-4">
        <div class="movies-display">
              <img src="{{this.image}}" alt="{{title}} picture">
              <p>{{this.title}}</p>
              <a href="/movies/{{this.id}}">See more</a>
        </div>

    </div>
    
      {{/each}}
  </div>
</div>
