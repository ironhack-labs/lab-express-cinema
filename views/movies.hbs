<div class="container">
  <h1>Ironhack Cinema</h1>
  <p>Click on a movie to check the showtimes!</p>
  <div class="row">
    {{#each movies}}
      <div class="col-md-4">
        <div class="card">
          <img class="card-img-top" src="{{this.image}}" alt="{{this.title}}"/>
          <div class="card-body">
            <h5 class="card-title">{{this.tile}}</h5>
            <a href="/movies/{{this._id}}">See more</a>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>