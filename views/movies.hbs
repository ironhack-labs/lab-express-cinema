<div>
    <h1>IRONHACK CINEMA</h1>
    <h3>Click on the movie to check the showtimes!</h3>
</div>
<div class="cards-container">
    {{#each movies}}
        <div class="movie-card">
            <img src="{{this.image}}"/>
            <h2>{{this.title}}</h2>
            <a href="/{{this._id}}">See More</a>
        </div>
    {{/each}}
</div>