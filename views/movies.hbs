<h1>Ironhack Cinema</h1>
<h2>Click on the movie to see the showtimes!</h2>
<section class="movies">
    {{#each this}}
    {{#with this}}
        {{#if image}}
            <article>   
                <img src="{{image}}" alt="film {{title}}">
                <h3>{{title}}</h3>
                <a href="/movies/{{_id}}">See more</a>
            </article>
        {{/if}}
    {{/with}}
    {{/each}}
</section>