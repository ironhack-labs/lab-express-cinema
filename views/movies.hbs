
<div class="title-wrapper">
<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>
</div>
<div class="movies-wrapper">
    {{#each movies}}
   <div class="movie-wrapper">
       <img src="{{this.image}}" alt="image">
       <br>
        <p>{{this.title}}</p>
        
        <a href="/movie/{{this._id}}">See more</a>
    </div>
    {{/each}}
</div>