<div id="movies">
<div>
<h1>Ironhack Cinema</h1>
<h2>Click on the movie to check the showtimes!</h2>
</div>

<div id="movies-cards">
{{#if movies}}
{{#each movies}}
<div id="card">
<img src="{{image}}">
<h3>{{title}}</h3>
<a href="/movies/{{_id}}">See more</a>
{{/each}}
</div>
{{/if}}
</div>
</div>
</div>