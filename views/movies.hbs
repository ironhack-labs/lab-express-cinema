<div class="moviePage">
    <h1>Ironhack Cinema</h1>
    <h2>Click on the movie to check the showtimes!</h2>
    <div class="moviePageContent">
        {{#each allMovies}}
        <div class="movieCard">
            <div class="movieCardContent">
                <img src="{{image}} alt=" Cover of the movie">
                <h3>{{title}}</h3>
                <a href="movies/{{id}}">See more</a>
            </div>

        </div>
        {{/each}}

    </div>

</div>