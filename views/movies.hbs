
<div class="title-wrapper">
<h1>This is the Movies page</h1>
</div>
<div class="movies-wrapper">
    {{#each movies}}
   <div class="movie-wrapper">
       <img src="{{this.image}}" alt="image">
       <br>
        <p>{{this.title}}</p>
        
        <a href="/movie/{{this._id}}">See more</a>
    </div>
    {{/each}}
</div>