<div class="movies">
    <h1>Ironhack Cinema</h1>
    <p>Click on the movie to check the showtimes!</p>

    <div class="movies-container">
        {{#each movies}}
        <div class="each-movie">
            <img src="{{this.image}}">
            <p>{{this.title}}</p>
            <a href="movies/{{_id}}">See more</a>
        </div>
        {{/each}}

    </div>
</div>