<div class="container">
    <h2>Check all the movies here!</h2>
    <div class="card">
        <div class="row">
            {{#each movie}}
            <img src="{{image}}" alt="images">
            <h3>{{title}}</h3>
            <hr>
            <p>{{description}}</p>
            <a href="/movies/{{_id}}">See More</a>
            <br>
            {{/each}}
        </div>
    </div>
</div>