<h1>Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>
<div id="container">
    {{#each moviesList}}
      <div class="movie-box">
        <img src="{{image}}" alt="{{title}}">
        <p><b>{{title}}</b></p>
        <a href="/movies/{{id}}">See more</a>
      </div>
    {{/each}}
</div>
