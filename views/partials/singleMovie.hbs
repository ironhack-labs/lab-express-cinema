
<div class="single-image-movie">
    <img src="{{image}}" alt="{{title}}">
</div>
    

<div class="detail-movie">
    <h1>{{title}}</h1>
    <h3>Director: {{director}}</h3>

    <h4>Stars</h4>
    <ul>
        {{#each stars}}
            <li>{{this}}</li>
         {{/each}}
    </ul>

    <p>{{description}}</p>

    <p>
        {{#each showtimes}}
        <span>{{this}} | </span>
        {{/each}}
    </p>
</div>



        