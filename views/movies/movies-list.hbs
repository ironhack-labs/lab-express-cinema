<div class="container mb-5">
    <h2 class=" mt-5">Ironhack Cinema</h2>
    <h3 class="mt-3 mb-5">Click on the movie to check the showtimes</h3>
    <div class="row ">


        {{#each allMovies}}
        <div class="col-md-4 text-center mb-5">
            <img src="{{this.image}}" alt="movie poster">
            <h5>{{this.title}}</h5>
            <a href="/movies/details/{{_id}}">See more</a>
        </div>
        {{/each}}

    </div>
</div>