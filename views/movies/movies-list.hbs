<div class="movies-list">
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check the showtimes!</p>
  <div class="movie-container">
    {{#each movies}}
      <div class="each-movie-container">
        <div>
          <img src="{{image}}" alt="{{title}}">
        </div>
        <h2>{{title}}</h2>
        <a href="/movie/{{_id}}">See more</a>
      </div>
    {{/each}}
  </div>
</div>