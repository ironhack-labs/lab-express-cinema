

   {{#each movies as |movie|}}
<div class="row">
  
   <div class=class="grid text-center" style="col-sm-6 mb-3 mb-sm-0">
  <img src={{image}} class="movie-image">
  <div class="card-body">
    <p class="card-text"> {{title}}</p>
    <a
      href="/movies/{{movie._id}}">
      See more
    </a> 
</div>

</div>
  {{/each}}
 

