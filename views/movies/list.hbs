<div>
  <h2 class="pb-4 fw-bolder">Ironhack Cinema</h2>
  <h2 class="pb-4">Click on the movies to check the showtimes</h2>
</div>
<div class='row row-cols-1 row-cols-md-4 g-2'>
  {{#each movies as |movie|}}
    <div class='col'>
      <div class='card border border-0'>
        {{> movie movie=movie}}
      </div>
    </div>
  {{/each}}
</div>