<div class='movies-wrapper'>

	<div class='movies'>
		{{#each data}}
			<div class='movie'>
				<img src='{{image}}' alt='{{title}}' />
				<h2>{{title}}</h2>
				<a href='/movies/{{_id}}'>See More</a>
			</div>
		{{/each}}

	</div>

</div>
<div class='back'>
	<a href='/'>Back</a>
</div>
