
<div class="card-detail">
  <img src={{image}} alt={{title}}>
  <div class="column-detail">
  <a href="/movies">Go back</a>
  <h2>{{title}}</h2>
  <h3>Director: {{director}}</h3>
  <h4>Stars:</h4>
    {{#each stars}}
    <li>{{this}}</li>
    {{/each}}
    
  <p>{{description}}</p>
  <p>
    {{#each showtimes}}
    {{this}} |
    {{/each}}
  </p>
  </div>
</div>
