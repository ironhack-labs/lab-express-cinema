<h2>Ironhack Cinema</h2>


<div class="details">
    <div><img class="caraturla" src="{{detailMovie.image}}" alt="{{detailMovie.name}}"></div>
    <div class="restDetails">
			<a href="/movies">Go Back </a>
			<h3>{{detailMovie.title}}</h3>
			<h4>Director: {{detailMovie.director}} </h4>
			<h5>Stars: </h5><ul>
					{{#each detailMovie.stars}}
					<li>{{this}}</li>
					{{/each}}
			</ul>
			<h5>{{detailMovie.description}}</h5>
			<p class="time">{{detailMovie.showtimes}}</p>
    </div>
    
</div>
