<h3>Ironhack Cinema</h3>
<p>Click on the movie to check the showtime!</p>

<div class = "container">
    <div class = "row">
{{#each movieList}}
    
  <div class = "col-4 margins">
    <img src = "{{this.image}}">
    <h6>{{this.title}}</h6>
    <a href = "/movies/{{this._id}}" >See more</a>
  </div>
 
    
{{/each}} 
</div> 
</div>

